<script src="~/lib/croppie/croppie.min.js"></script>

<script>
        var crop;
        const country = '@(user.CompanySetup && user.CompanyModel.Country != null ? user.CompanyModel.Country : "")';
        if (country.length > 0) {
            $(document).ready(function() {
                $("#Country").val(country);
            });
        }
        function readFile() {

            if (this.files && this.files[0]) {

                var FR = new FileReader();

                FR.addEventListener("load", function (e) {
                    if (crop !== undefined) {
                        $('#crop-image').croppie('bind', {
                            url: e.target.result
                        });
                    } else {
                        crop = $('#crop-image').croppie({
                            url: e.target.result,
                            viewport: { width: 180, height: 180 },
                            boundary: { width: 300, height: 300 },
                        });
                    }
                });

                FR.readAsDataURL(this.files[0]);
            }

        }
        $('#update-profile').on('click', function (ev) {
            if (crop != undefined) {
                crop.croppie('result', {
                    type: 'canvas',
                    size: 'viewport'
                }).then(function (blob) {
                    $('#imagebase64').val(blob);
                    $('#account-settings-form').submit();
                });
            } else {
                $('#account-settings-form').submit();
            }
        });
        document.getElementById("CompanyImage").addEventListener("change", readFile);
    </script>