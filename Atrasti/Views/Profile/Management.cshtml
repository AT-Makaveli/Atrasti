                                           class="form-control" accept="image/*" style="padding-bottom: 35px !important;">
                                    <input type="hidden" id="imagebase64" asp-for="CompanyImage">
                                    <div id="crop-image"></div>
                                </div>
                            </div>
                            <!-- /.box-body -->

